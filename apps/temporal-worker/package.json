{
  "name": "temporal-worker",
  "version": "1.0.0",
  "description": "Temporal worker for bloop project",
  "main": "dist/worker.js",
  "scripts": {
    "build": "tsc",
    "dev": "nodemon",
    "start": "node dist/worker.js",
    "db:wait": "pnpm --filter @bloop/database db:wait",
    "db:init": "pnpm --filter @bloop/database db:init",
    "db:generate": "pnpm --filter @bloop/database db:generate",
    "db:migrate": "pnpm --filter @bloop/database db:migrate",
    "db:push": "pnpm --filter @bloop/database db:push",
    "db:studio": "pnpm --filter @bloop/database db:studio",
    "dev:with-init": "pnpm --filter @bloop/shared-types build && pnpm --filter @bloop/database build && pnpm db:wait && pnpm db:init && pnpm dev"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@bloop/database": "workspace:*",
    "@bloop/shared-types": "workspace:*",
    "@temporalio/activity": "^1.12.2",
    "@temporalio/client": "^1.12.2",
    "@temporalio/common": "^1.12.2",
    "@temporalio/worker": "^1.12.2",
    "@temporalio/workflow": "^1.12.2",
    "cheerio": "^1.1.2",
    "date-fns": "^4.1.0",
    "https-proxy-agent": "^7.0.6",
    "pornhub.js": "^1.14.1",
    "undici": "^7.18.2"
  },
  "devDependencies": {
    "@types/node": "^24.1.0",
    "nodemon": "^3.1.10",
    "tsx": "^4.20.3",
    "typescript": "^5.8.3"
  }
}